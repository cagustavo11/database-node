    "method-override": "^3.0.0",
    "nanoid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/hbs": "^4.0.1",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "copyfiles": "^2.4.1",
    "morgan": "^1.10.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
